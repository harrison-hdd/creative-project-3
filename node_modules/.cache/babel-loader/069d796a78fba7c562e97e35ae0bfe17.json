{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n\"use strict\";\n\nimport \"core-js/modules/es.string.pad-start.js\";\nimport \"core-js/modules/es.array.concat.js\";\nexport default {\n  name: \"ImageBrowser\",\n  created: function created() {\n    this.setup();\n  },\n  props: {\n    mediaType: String,\n    mediaTitle: String,\n    mediaURL: String,\n    explanation: String\n  },\n  data: function data() {\n    return {\n      apiKey: \"5NqF6jh9w3uVhB9rwEeUJfSIwBgcavPvzYEO7lH3\"\n    };\n  },\n  methods: {\n    setup: function setup() {\n      var now = new Date();\n      var dd = String(now.getDate()).padStart(2, \"0\");\n      var mm = String(now.getMonth() + 1).padStart(2, \"0\"); //January is 0!\n\n      var yyyy = now.getFullYear();\n      var today = \"\".concat(yyyy, \"-\").concat(mm, \"-\").concat(dd); // eslint-disable-next-line vue/no-mutating-props\n\n      this.$root.$data.currentDate = today;\n      var maxDateAttr = document.createAttribute(\"max\");\n      var valueAttr = document.createAttribute(\"value\");\n      maxDateAttr.value = today;\n      valueAttr.value = today;\n      document.getElementById(\"search-box\").setAttributeNode(maxDateAttr);\n      document.getElementById(\"search-box\").setAttributeNode(valueAttr); // eslint-disable-next-line vue/no-mutating-props\n\n      this.mediaType = \"image\";\n    },\n    updateDate: function updateDate(event) {\n      event.preventDefault();\n      var selectedDate = document.getElementById(\"search-box\").value;\n      this.$root.$data.currentDate = selectedDate;\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmEA;;;;AAEA;AACAA,sBADA;AAEAC,SAFA,qBAEA;AACA;AACA,GAJA;AAKAC;AACAC,qBADA;AAEAC,sBAFA;AAGAC